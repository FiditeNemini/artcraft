{
    "info": {
        "name": "Voice Designer API",
        "description": "Endpoints for the Voice Designer API",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
        {
            "name": "Voice Designer - Datasets",
            "item": [
                {
                    "name": "Create Dataset",
                    "request": {
                        "method": "POST",
                        "url": "{{base_url}}/v1/voice_designer/datasets/create"
                    }
                },
                {
                    "name": "Update Dataset",
                    "request": {
                        "method": "POST",
                        "url": "{{base_url}}/v1/voice_designer/datasets/{{dataset_token}}/update"
                    }
                },
                {
                    "name": "Delete Dataset",
                    "request": {
                        "method": "DELETE",
                        "url": "{{base_url}}/v1/voice_designer/datasets/{{dataset_token}}/delete"
                    }
                },
                {
                    "name": "List Datasets By User",
                    "request": {
                        "method": "GET",
                        "url": "{{base_url}}/v1/voice_designer/datasets/user/{{user_token}}/list"
                    }
                }
            ]
        },
        {
            "name": "Voice Designer - Voice",
            "item": [
                {
                    "name": "Create Voice",
                    "request": {
                        "method": "POST",
                        "url": "{{base_url}}/v1/voice_designer/voice/create"
                    }
                },
                {
                    "name": "Update Voice",
                    "request": {
                        "method": "POST",
                        "url": "{{base_url}}/v1/voice_designer/voice/{{voice_token}}/update"
                    }
                },
                {
                    "name": "Delete Voice",
                    "request": {
                        "method": "DELETE",
                        "url": "{{base_url}}/v1/voice_designer/voice/{{voice_token}}/delete"
                    }
                },
                {
                    "name": "List Voices By User",
                    "request": {
                        "method": "GET",
                        "url": "{{base_url}}/v1/voice_designer/voice/user/{{user_token}}/list"
                    }
                }
            ]
        },
        {
            "name": "Voice Designer - Sample",
            "item": [
                {
                    "name": "Upload Sample",
                    "request": {
                        "method": "POST",
                        "url": "{{base_url}}/v1/voice_designer/sample/upload"
                    }
                },
                {
                    "name": "Delete Sample",
                    "request": {
                        "method": "DELETE",
                        "url": "{{base_url}}/v1/voice_designer/sample/delete"
                    }
                },
                {
                    "name": "List Samples By Dataset",
                    "request": {
                        "method": "GET",
                        "url": "{{base_url}}/v1/voice_designer/sample/data_set/{{data_set_token}}/list"
                    }
                }
            ]
        },
        {
            "name": "Voice Designer - Inference",
            "item": [
                {
                    "name": "Enqueue TTS Request",
                    "request": {
                        "method": "POST",
                        "url": "{{base_url}}/v1/voice_designer/inference/enqueue_tts"
                    }
                },
                {
                    "name": "Enqueue VC Request",
                    "request": {
                        "method": "POST",
                        "url": "{{base_url}}/v1/voice_designer/inference/enqueue_vc"
                    }
                }
            ]
        },
        {
            "name": "Voice Designer - Inventory",
            "item": [
                {
                    "name": "List Available Voices",
                    "request": {
                        "method": "GET",
                        "url": "{{base_url}}/v1/voice_designer/inventory/list"
                    }
                },
                {
                    "name": "List User Models",
                    "request": {
                        "method": "GET",
                        "url": "{{base_url}}/v1/voice_designer/inventory/user_list"
                    }
                },
                {
                    "name": "List Favorite Models",
                    "request": {
                        "method": "GET",
                        "url": "{{base_url}}/v1/voice_designer/inventory/favorites_list"
                    }
                },
                {
                    "name": "Search Voices",
                    "request": {
                        "method": "GET",
                        "url": "{{base_url}}/v1/voice_designer/inventory/search"
                    }
                }
            ]
        }
    ]
}
