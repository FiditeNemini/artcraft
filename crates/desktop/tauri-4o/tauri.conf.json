{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "ArtCraft",
  "version": "0.1.0",
  "identifier": "com.unique.value.thing.why",
  "build": {
    "frontendDist": "../../../frontend/realtime/dist",
    "devUrl": "http://localhost:5741",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build2"
  },
  "app": {
    "windows": [
      {
        "title": "ArtCraft",
        "width": 2400,
        "height": 1300,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
